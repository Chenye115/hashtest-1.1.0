{
"update_url": "http://chenye2000.top",

  "name": "Hashcash",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Calculates hashcash and post reponse",
  "offline_enabled": true,
  "permissions": [
		"clipboardWrite", "tabs",
		"webRequest",  
		"webRequestBlocking", 
		"storage", 
		"http://*/*", 
		"https://*/*" 
  ],
  "browser_action": {
    "default_icon": "images/hashcash.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "icons": {
    
  }
}
